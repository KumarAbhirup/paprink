{
  "version": 2,
  "name": "paprink",
  "public": true,
  "alias": [
    "paprinkNowv2"
  ],
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    },
    {
      "src": "next.config.js",
      "use": "@now/next"
    },
    { 
      "src": "static/*", 
      "use": "@now/static" 
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "$1",
      "continue": true
    }
  ]
}